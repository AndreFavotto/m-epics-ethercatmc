# The Name of the system
record(stringin, "$(SYSPFX)PlcName") {
    field(DTYP, "asynOctetRead")
    field(INP, "@asyn($(MOTOR_PORT),0)MOTOR_MESSAGE_TEXT")
    field(SCAN, "I/O Intr")
}


# The IPADDR of the MCU
record(stringout, "$(SYSPFX)IPADDR") {
    field(VAL, "$(IPADDR)")
}

# The AMSNETID of the IOC
record(stringout, "$(SYSPFX)AMSNETIDIOC") {
    field(VAL, "$(AMSNETIDIOC)")
}


# The IPPORT of the system
record(stringout, "$(SYSPFX)IPPORT") {
    field(VAL, "$(IPPORT)")
}


record (asyn, "$(SYSPFX)$(MOTOR_PORT):asyn")
{
}
